# Environment Configuration

# API Configuration
API_BASE_URL=https://api.cipherx.io/v1
API_RATE_LIMIT=1000

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/cipherx
DATABASE_POOL_MIN=2
DATABASE_POOL_MAX=10

# Redis
REDIS_URL=redis://localhost:6379
REDIS_TTL=3600

# JWT Authentication
JWT_SECRET=your_super_secret_jwt_key_change_in_production
JWT_EXPIRY=24h

# Telegram Bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_WEBHOOK_URL=https://your-domain.com/webhook

# Solana Configuration
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_NETWORK=mainnet-beta

# Helius API
HELIUS_API_KEY=your_helius_api_key_here
HELIUS_WEBHOOK_SECRET=your_webhook_secret

# Chainlink Oracle
CHAINLINK_RPC_URL=your_chainlink_rpc_url

# AWS Services (Production)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
S3_BUCKET=cipherx-storage

# Monitoring & Logging
SENTRY_DSN=your_sentry_dsn_here
LOG_LEVEL=info

# Features Flags
ENABLE_AI_SIGNALS=true
ENABLE_WHALE_TRACKING=true
ENABLE_NOTIFICATIONS=true

# Rate Limiting
FREE_TIER_LIMIT=60
PRO_TIER_LIMIT=600
ENTERPRISE_TIER_LIMIT=0

# AI Model Configuration
MODEL_CONFIDENCE_THRESHOLD=0.7
MAX_SIGNALS_PER_HOUR=10
MODEL_UPDATE_INTERVAL=3600
